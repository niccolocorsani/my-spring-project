
version: '3.3'
services:

  db:
    image: mysql:latest
   # volumes:
   #   - db5:/var/lib/mysql

    #container_name: mysql-db
    environment:
      - MYSQL_ROOT_PASSWORD=user
      - MYSQL_USER=user
    ports:
      - "3307:3306"



  customerservice:
    image: customer-service
    #container_name: customer-service-app
    build:
      context: .
      dockerfile: Dockerfile
    ports:
      - "8080:8080"

    depends_on:
      - db
    environment:
      SPRING_PROFILES_ACTIVE: MySQLdocker

#volumes: #Dichiarazione del volume, obbligatoria
 # db5: